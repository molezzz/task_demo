<h1>Listing todos</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Project</th>
      <th>Creator</th>
      <th>Owner</th>
      <th>Content</th>
      <th>End at</th>
      <th>Complate at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todos.each do |todo| %>
      <tr>
        <td><%= todo.key %></td>
        <td><%= todo.project %></td>
        <td><%= todo.creator_id %></td>
        <td><%= todo.owner_id %></td>
        <td><%= todo.content %></td>
        <td><%= todo.end_at %></td>
        <td><%= todo.complate_at %></td>
        <td><%= link_to 'Show', todo %></td>
        <td><%= link_to 'Edit', edit_todo_path(todo) %></td>
        <td><%= link_to 'Destroy', todo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Todo', new_todo_path %>
